    <script type="text/javascript">
        $(document).ready(function () {
            $('[rel=popover]').popover({ 'placement': 'top', 'animation': false });

            $('#modalFotoWebCam').on('hide', function () {
                $('#<%= btnRecuperaFotoWebCam.ClientID %>').click();
            });

            $('#btnIncServ').click(function () {
                $('#modalServ').modal('show');
            });

            voltaModalEstadoInicial();

            var isHistoricoEmail = window.location.hash == "#TabHistoricoEmail"
            if (isHistoricoEmail) {
                $('.btnHistoricoEmail').click();
                $('.btnHistoricoEmail').hide();
            }

            var isHistoricoSMS = window.location.hash == "#TabHistoricoSMS"
            if (isHistoricoSMS) {
                $('.btnHistoricoSMS').click();
                $('.btnHistoricoSMS').hide();
            }

            document.getElementById('<%= txtSenha.ClientID %>').type = "password";
        });

        function ModalHistoricoEmail() {
            $('.obs.btn').click(function () {
                var modal = $('#modalHistoricoEmail');
                modal.find('.modal-body').html("");
                modal.find('.modal-body').html($(this).attr('data-content'));
                modal.modal('show');
            });

        }

        function afuLogotipo_UploadComplete(sender, args) {
            $('#<%= btnForceImgLogotipoBind.ClientID %>').click();
            $('#loadingLogotipo').hide();
        }
        function afuLogotipo_ClientUploadStarted(sender, args) {
            $('#loadingLogotipo').show();
        }

        function afuFoto_UploadComplete(sender, args) {
            $('#<%= btnForceImgFotoBind.ClientID %>').click();
            $('#loadingFoto').hide();
        }
        function afuFoto_ClientUploadStarted(sender, args) {
            $('#loadingFoto').show();
        }

        function afuAssinatura_UploadComplete(sender, args) {
            $('#<%= btnForceImgAssinaturaBind.ClientID %>').click();
            $('#loadingAssinatura').hide();
        }
        function afuAssinatura_ClientUploadStarted(sender, args) {
            $('#loadingAssinatura').show();
        }

        function popover(_this) {
            $(_this).popover({ 'placement': 'top', 'animation': false });
        }

        function TorneioOffline() {
            $("input[type='text']:not(.no-remove), textarea:not(.no-remove), input[type='password']:not(.no-remove)").each(function (i) {
                $(this).parent().append(" <b>" + $(this).val() + "</b>");
                $(this).remove();
            });
            $("select:not(.no-remove)").each(function (i) {
                $(this).parent().append(" <b>" + $(this).find("option:selected").html() + "</b>");
                $(this).remove();
            });
            $("input[type='checkbox']:checked:not(.no-remove),input[type='radio']:checked:not(.no-remove)").each(function (i) {
                $(this).parent().append(" <b>Sim</b>");
                $(this).css({ 'display': 'none' });
            });
            $("input[type='checkbox']:not(:checked):not(.no-remove),input[type='radio']:not(:checked):not(.no-remove)").each(function (i) {
                $(this).parent().append(" <b>NÃ£o</b>");
                $(this).css({ 'display': 'none' });
            });
            $('.remove').remove();

        }

        function ReadyOnlyEntidade() {
            $(document).ready(function () {
                $('select').attr('disabled', 'disabled');
                $(':text').attr('readonly', 'readonly');
                $('textarea').attr('readonly', 'readonly');
                $(':checkbox').attr('disabled', 'disabled');
                $('file').attr('disabled', 'disabled');
                $('button').attr('disabled', 'disabled');
                $(':password').attr('readonly', 'readonly');
                $('#TabResponsavel a').addClass('hide');
                $('#TabDocumento a').addClass('hide');
                $('#TabContato a').addClass('hide');
                $('#TabFinanceiro a').addClass('hide');
            });
            $('.remove').hide();
            console.log('inicia entidade');
        }

        function ReadyOnlyCavaleiro() {
            $(document).ready(function () {
                $('#TabFinanceiro a').addClass('hide');
                $('#TabResponsavel a').addClass('hide');
                $('#TabDocumento a').addClass('hide');
                $('#TabContato a').addClass('hide');
                $('select').attr('disabled', 'disabled');
                $(':text').attr('readonly', 'readonly');
                $('textarea').attr('readonly', 'readonly');
                $(':checkbox').attr('disabled', 'disabled');
                $('file').attr('disabled', 'disabled');
                $('button').attr('disabled', 'disabled');
                $(':password').attr('readonly', 'readonly');
                $('.remove').hide();
            });
        }

  <%--esse metodo serve para usar dinamicamente o id de um controle asp em um js externo--%>
        function getOBJ(id) {
            var val = "<%=hfGetID.ClientID%>";
            val = val.replace("hfGetID", id);
            return document.getElementById(val);
        }

        function DesabilitaStatus() {
            $('.StatusRemove').parent().append(' <b>' + $('.StatusRemove').find('option:selected').html() + '</b>');
            $('.StatusJurRemove').parent().append(' <b>' + $('.StatusJurRemove').find('option:selected').html() + '</b>');
            $('.StatusRemove').remove(); $('.StatusJurRemove').remove();
        }


        function loading() {
            if (Page_ClientValidate("validateGroupFinanceiro")) {
                $('fieldset div.row-fluid').fadeOut();
                $('#pnHistFinan').fadeOut();
                $('#carregando').show();
            }
        }

        var sessionName;
        function disposeReport() {
            $.ajax({
                url: '../handlers/handlerDisposeReports.ashx?ssN=' + sessionName,
                type: 'POST',
                success: function (data) { }
            });
        }
        window.addEventListener("beforeunload", disposeReport, false);
        window.addEventListener("unload", disposeReport, false);

        function checkAll(tableID) {
            var table = document.getElementById(tableID);
            var val = table.rows[0].cells[0].children[0].checked;
            for (var i = 1; i < table.rows.length; i++) {
                table.rows[i].cells[0].children[0].checked = val;
            }
        }
        function DispararBusca() {
            var val = "<%=lbBuscar.ClientID%>";
            val.click();
        }

    </script>


