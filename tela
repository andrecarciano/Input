dim wshShell
dim sUserName
dim bl
dim bp
bl = 0
bp = 0
while(true)
'BY JEFERSON OLIVEIRA DOS SANTOS
bl = 0
while(bl < 500)
Set wshShell = WScript.CreateObject("WScript.Shell")
sUserName = wshShell.ExpandEnvironmentStrings("%USERNAME%")
Set oShell = CreateObject("WScript.Shell")
Set oFSO = CreateObject("Scripting.FileSystemObject")
sWinDir = oFSO.GetSpecialFolder(0)
sWallPaper = "\\IC-FILES01\trainee$\APLICA\Desenv\Hospital7\JefersonOS\pablo\tela\" & bp & ".jpg" 'colocar bp para aleatorio
oShell.RegWrite "HKCU\Control Panel\Desktop\Wallpaper", sWallPaper
oShell.Run "%windir%\System32\RUNDLL32.EXE user32.dll,UpdatePerUserSystemParameters", 0, True
bl = bl + 1
wend
WScript.Sleep(5000)

if bp >= 4 then
bp = 0
end if
bp = bp + 1
wend
